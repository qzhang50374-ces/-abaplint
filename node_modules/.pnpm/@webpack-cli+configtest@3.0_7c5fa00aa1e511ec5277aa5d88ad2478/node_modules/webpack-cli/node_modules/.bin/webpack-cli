#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/q_zhang50374/Documents/abaplint/abaplint/web/playground/node_modules/.pnpm/webpack-cli@6.0.1_webpack-dev-server@5.2.3_webpack@5.104.1/node_modules/webpack-cli/bin/node_modules:/mnt/c/Users/q_zhang50374/Documents/abaplint/abaplint/web/playground/node_modules/.pnpm/webpack-cli@6.0.1_webpack-dev-server@5.2.3_webpack@5.104.1/node_modules/webpack-cli/node_modules:/mnt/c/Users/q_zhang50374/Documents/abaplint/abaplint/web/playground/node_modules/.pnpm/webpack-cli@6.0.1_webpack-dev-server@5.2.3_webpack@5.104.1/node_modules:/mnt/c/Users/q_zhang50374/Documents/abaplint/abaplint/web/playground/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/q_zhang50374/Documents/abaplint/abaplint/web/playground/node_modules/.pnpm/webpack-cli@6.0.1_webpack-dev-server@5.2.3_webpack@5.104.1/node_modules/webpack-cli/bin/node_modules:/mnt/c/Users/q_zhang50374/Documents/abaplint/abaplint/web/playground/node_modules/.pnpm/webpack-cli@6.0.1_webpack-dev-server@5.2.3_webpack@5.104.1/node_modules/webpack-cli/node_modules:/mnt/c/Users/q_zhang50374/Documents/abaplint/abaplint/web/playground/node_modules/.pnpm/webpack-cli@6.0.1_webpack-dev-server@5.2.3_webpack@5.104.1/node_modules:/mnt/c/Users/q_zhang50374/Documents/abaplint/abaplint/web/playground/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli.js" "$@"
else
  exec node  "$basedir/../../bin/cli.js" "$@"
fi
